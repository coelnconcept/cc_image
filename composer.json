{
    "name": "coelnconcept/cc-image",
    "type": "typo3-cms-extension",
    "description": "Provide image viewhelpers and support for WebP",
    "authors": [],
    "license": "GPL-3.0-or-later",
    "require": {
        "typo3/cms-core":  ">=11.5 <14.0"
    },
    "autoload": {
        "psr-4": {
            "CoelnConcept\\CcImage\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/cc-image": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "cc_image"
        }
    }
}